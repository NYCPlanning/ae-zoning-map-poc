{
  "name": "react-vite-boilerplate",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint ./src --ext ts,tsx,js,jsx",
    "lint:fix": "eslint ./src --ext ts,tsx,js,jsx --fix",
    "typecheck": "tsc",
    "preview": "vite preview",
    "prepare": "husky install",
    "generate": "kubb generate",
    "test": "vitest"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.1.1",
    "@deck.gl/extensions": "^8.9.33",
    "@deck.gl/geo-layers": "^8.9.32",
    "@deck.gl/react": "^8.9.31",
    "@nycplanning/streetscape": "^0.9.0",
    "@tanstack/react-query": "^4.36.1",
    "@turf/centroid": "^6.5.0",
    "axios": "^1.6.1",
    "maplibre-gl": "^3.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-map-gl": "^7.1.6",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@jest/globals": "^29.7.0",
    "@kubb/cli": "^2.6.6",
    "@kubb/core": "^2.6.6",
    "@kubb/swagger": "^2.6.6",
    "@kubb/swagger-faker": "^2.6.6",
    "@kubb/swagger-msw": "^2.6.6",
    "@kubb/swagger-tanstack-query": "^2.6.6",
    "@kubb/swagger-ts": "^2.6.6",
    "@testing-library/react": "^14.2.1",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@types/turf": "^3.5.32",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.2",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "vite": "^4.3.9",
    "vitest": "^1.3.1"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "bash -c 'npm run typecheck'",
      "npm run lint -- --fix"
    ]
  }
}
